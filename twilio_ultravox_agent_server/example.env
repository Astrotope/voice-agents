# Copy this file to .env and fill in your actual values

# Ultravox API Key (get from https://app.ultravox.ai)
ULTRAVOX_API_KEY=your_ultravox_api_key_here

# Ultravox Corpus ID (run setup-corpus.ts to get this)
ULTRAVOX_CORPUS_ID=your_corpus_id_here

# Twilio credentials for call management and transfers
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here

# Human agent phone number for transfers
HUMAN_AGENT_PHONE=+1234567890

# Server configuration
PORT=3000
BASE_URL=http://localhost:3000

# For production, set to your actual server URL
# BASE_URL=https://your-domain.com

# Optional: Set environment
NODE_ENV=development
